{
  "name": "preactioncms-blockext-ytvid",
  "description": "Preaction CMS Youtube Video Extension Block",
  "repository": "https://github.com/duhdugg/preactioncms-blockext-ytvid.git",
  "author": "Doug Elkin <doug@dougelkin.com>",
  "license": "MIT",
  "version": "0.0.1",
  "private": true,
  "module": "dist/preactioncms-blockext-ytvid.esm.js",
  "files": [
    "dist/*",
    "src/*",
    "public/*"
  ],
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:prettier/recommended",
      "plugin:react/recommended"
    ]
  },
  "prettier": {
    "jsxSingleQuote": true,
    "semi": false,
    "singleQuote": true
  },
  "browserslist": {
    "production": "defaults and supports es6-module",
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "prop-types": "^15.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@popperjs/core": "^2.11.6",
    "@preaction/bootstrap-clips": "^5.0.0-alpha.23",
    "@preaction/inputs": "^5.0.0-alpha.12",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^14.1.0",
    "@rollup/plugin-sucrase": "^4.0.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "bootstrap": "^5.2.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "rollup": "^2.79.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-visualizer": "^5.8.1",
    "sass": "^1.55.0"
  },
  "peerDependencies": {
    "@preaction/bootstrap-clips": "^5.0.0-alpha.23",
    "@preaction/inputs": "^5.0.0-alpha.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "scripts": {
    "start": "DISABLE_ESLINT_PLUGIN=true BROWSER=none react-scripts start",
    "build": "rollup -c",
    "test": "npm run prettier-check && npm run eslint-check && react-scripts test --watchAll=false",
    "eject": "react-scripts eject",
    "prettier-check": "prettier -c ./*.json ./*.js src/*.j* src/*.scss public/*.html",
    "eslint-check": "eslint --max-warnings 0 ./*.js src/*.j*"
  }
}
